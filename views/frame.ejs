<!doctype html>
<html>

<head>
    <meta http-equiv="refresh" content="3600">
    <title>Dashboard</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.0/css/materialize.min.css">
    <style>
        html,
        body,
        iframe {
            width: 100%;
            height: 100%;
            margin: 0;
            padding: 0;
            overflow: hidden;
        }
    </style>
</head>

<body>
    <iframe id="foo" frameborder="0" scrolling="no" border="0"></iframe>
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script>
        var urls = [{
            url: '<%= static ? "/home.html": "/home" %>',
            time: 30000
        }];
        var i = 0;
        var l = urls.length;
        var to;

        function rotate() {
            var e = document.getElementById('foo');
            if (i != l - 1) {
                i++
            } else {
                i = 0;
            }
            e.src = urls[i].url;
            clearTimeout(to);
            to = setTimeout(rotate, urls[i].time);
        }
        $(function() {
            rotate();
        });
    </script>
</body>

</html>